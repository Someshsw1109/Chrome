{
    "name": "linkedin-connection-automation",
    "description": "Automatically send connection requests on LinkedIn with a click.",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "wxt",
        "build": "tsc",
        "dev:firefox": "wxt -b firefox",
        "build:firefox": "wxt build -b firefox",
        "zip": "wxt zip",
        "zip:firefox": "wxt zip -b firefox",
        "compile": "tsc --noEmit",
        "postinstall": "wxt prepare"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "webextension-polyfill": "^0.12.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "@types/webextension-polyfill": "^0.12.1",
        "@wxt-dev/module-react": "^1.1.1",
        "typescript": "^5.6.2",
        "wxt": "^0.19.11"
    }
}
